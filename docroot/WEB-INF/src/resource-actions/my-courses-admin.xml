<?xml version="1.0" encoding="UTF-8"?>

<resource-action-mapping>

  <portlet-resource>
    <portlet-name>My-Courses-Admin</portlet-name>
    <permissions>
      <supports>
        <action-key>ADD_ENTITY</action-key>
        <action-key>VIEW</action-key>
      </supports>
      <community-defaults>
        <action-key>VIEW</action-key>
      </community-defaults>
      <guest-defaults />
      <guest-unsupported>
      	<action-key>ADD_ENTITY</action-key>
      </guest-unsupported>
    </permissions>
  </portlet-resource>

  <model-resource>
    <model-name>com.gnenc.internet.mycourses.model.Entity
    </model-name>
    <portlet-ref>
      <portlet-name>My-Courses-Admin</portlet-name>
    </portlet-ref>
    <permissions>
      <supports>
        <action-key>DELETE</action-key>
        <action-key>UPDATE</action-key>
        <action-key>VIEW</action-key>
      </supports>
      <community-defaults/>
      <guest-defaults/>
      <administrator-defaults>
      	<action-key>DELETE</action-key>
        <action-key>UPDATE</action-key>
      </administrator-defaults>
      <guest-unsupported>
      	<action-key>DELETE</action-key>
        <action-key>UPDATE</action-key>
        <action-key>VIEW</action-key>
      </guest-unsupported>
    </permissions>
  </model-resource>

</resource-action-mapping>