<?xml version="1.0" encoding="UTF-8"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="org.gnenc.internet.mycourses.model.impl.CourseModelImpl">
		<attributes>
			<id name="id">
				<column name="ID_" />
			</id>
			<basic name="courseId" />
			<basic name="name" />
			<basic name="entityId" />
			<transient name="primaryKey" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="org.gnenc.internet.mycourses.model.impl.EntityModelImpl">
		<attributes>
			<id name="entityId" />
			<basic name="entityName" />
			<basic name="url" />
			<basic name="emailDomains" />
			<basic name="dbName" />
			<transient name="primaryKey" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="org.gnenc.internet.mycourses.model.impl.UserEnrollmentModelImpl">
		<attributes>
			<id name="id">
				<column name="ID_" />
			</id>
			<basic name="userId" />
			<basic name="courseId" />
			<basic name="lastRefresh">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<transient name="primaryKey" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="org.gnenc.internet.mycourses.model.impl.HostModelImpl">
		<attributes>
			<id name="hostId" />
			<basic name="entityId" />
			<basic name="remoteEntityId" />
			<basic name="peerId" />
			<transient name="primaryKey" />
		</attributes>
	</mapped-superclass>
	<entity class="org.gnenc.internet.mycourses.model.impl.CourseImpl" name="Course">
		<table name="MC_Course" />
		<attributes />
	</entity>
	<entity class="org.gnenc.internet.mycourses.model.impl.EntityImpl" name="Entity">
		<table name="MC_Entity" />
		<attributes />
	</entity>
	<entity class="org.gnenc.internet.mycourses.model.impl.UserEnrollmentImpl" name="UserEnrollment">
		<table name="MC_UserEnrollment" />
		<attributes />
	</entity>
	<entity class="org.gnenc.internet.mycourses.model.impl.HostImpl" name="Host">
		<table name="MC_Host" />
		<attributes />
	</entity>
</entity-mappings>