<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_0_0.dtd">

<service-builder package-path="org.gnenc.internet.mycourses">
	<author>Drew Blessing/Stephen Hunter</author>
	
	<namespace>MC</namespace>

	<entity name="Course" local-service="true" remote-service="false">
	
		<column name="courseId" type="long" primary="true" />
		
		<column name="name" type="String" />
		
		<column name="entityId" type="Collection" entity="Entity" mapping-key="entityId" />
		<column name="lastRefresh" type="long" />
		
		<order by="asc">
			<order-column name="name" case-sensitive="false" />
			<order-column name="entityId" />
		</order>
		
		<!--Service Builder automatically inserts a finder for the primary key-->
		
		<finder name="EntityId" return-type="Collection">
			<finder-column name="entityId" />
		</finder>
		
		
	</entity>
	
	
	<entity name="Entity" local-service="true" remote-service="false">
	
		<column name="entityId" type="long" primary ="true" />
		
		<column name="entityName" type="String" />
		<column name="url" type="String" />
		
		<column name="emailDomains" type="String"/>
		<column name="dbName" type="String" />
		
		<order by="asc">
			<order-column name="entityName"/>
		</order>
		
		<finder name="EmailDomain" return-type="Collection">
			<finder-column name="emailDomains" />
		</finder>
		
		<finder name="URL" return-type="Collection">
			<finder-column name="url" />
		</finder>
	
	
	</entity>
	
</service-builder>